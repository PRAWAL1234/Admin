{
    "version":2,
    "builds":[
        {
            "src":"admin/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSiz":"15mb","runtime":"python3.9"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles-build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"admin/wsgi.py"
        }
    ]
}